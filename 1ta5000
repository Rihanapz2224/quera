# تابع برای پیدا کردن
def digit(k):
    # ساختن رشته‌ای شامل اعداد 1 تا 5000
    num_str = ''.join(map(str, range(1, 5001)))
    
    # چاپ رقم k ام از رشته (توجه به اینکه اندیس‌ها از 0 شروع می‌شوند)
    print(num_str[k-1])

# گرفتن مقدار k از کاربر
k = int(input(""))

# فراخوانی تابع برای پیدا کردن و چاپ رقم k ام
_digit(k)
