# تابع چاپ جدول ضرب
def table(n):
    # حلقه برای ‌سطر‌ها از 1 تا ان
    for i in range(1, n+1):
        # حلقه برای ستون‌ها از 1 تا ان
        for j in range(1, n+1):
            # چاپ نتیجه ضرب i و j
             print(i * j, end='\t')
                #   از تب برای فاصله استفاده میکنیم
                  
        # چاپ یک خط جدید بعد از هر ردیف
        print()

# گرفتن ورودی از کاربر
n = int(input())

# تابع رو برای چاپ جدول ضرب صدا میکنیم
table(n)
